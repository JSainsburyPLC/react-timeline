{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/env", {
        "targets": {
          "browsers": ["last 2 versions", "ie 9-11"],
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    "add-module-exports",
    "@babel/transform-runtime",
    "@babel/plugin-proposal-class-properties"
  ]
}
